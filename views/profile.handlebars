<div class="row">
  <div class="col-auto">
    <h2>Welcome, {{name}}!</h2>
  </div>
</div>

<div class="col-md-6 project-list">
    <h3>Email Templates:</h3>

    {{#each emails as |email|}}
    <div class="row mb-2">
      <div class="col-md-8">
        <h4><a href="/email/{{email.id}}">{{email.name}}</a></h4>
      </div>
    </div>
    {{/each}}
  </div>
</div>

<script src="./js/profile.js"></script>


